{
    "name": "gemadigital/file-manager",
    "description": "Gema Digital File Manager",
    "license": "MIT",
    "authors": [
        {
            "name": "António Almeida",
            "email": "promatik@gmail.com",
            "homepage": "https://promatik.pt"
        },
        {
            "name": "Mário Santos",
            "email": "mariogilgsantos@gmail.com",
            "homepage": "https://www.linkedin.com/in/m%C3%A1rio-santos-dev/"
        }
    ],
    "homepage": "https://github.com/gema/laravel-file-manager",
    "keywords": [
        "Laravel",
        "FileManager"
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "illuminate/support": "~7|~8|~9|~10"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "GemaDigital\\FileManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GemaDigital\\FileManager\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GemaDigital\\FileManager\\FileManagerServiceProvider"
            ],
            "aliases": {
                "FileManager": "GemaDigital\\FileManager\\Facades\\FileManager"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
